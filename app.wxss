/**app.wxss**/
/*引入weui样式*/
@import 'weui.wxss';

page {
  height: 100%;
}

.body {
  width: 100%;
  height: 100%;
  background-color: #ebebeb;
}

.location-text {
  color: #888;
  padding: 50rpx 0 20rpx 50rpx;
  font-size: 13pt;
}

.location-content {
  color: #000;
  height: 95rpx;
  background: #fff;
  padding: 0 50rpx 0 50rpx;
  line-height: 95rpx;
  font-size: 14pt;
}

.flex_between {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.listArea {
  padding-top: 40rpx;
}

.bb {
  border-bottom: 1px solid #d9d9d9;
}

.block {
  height: 40rpx;
}
